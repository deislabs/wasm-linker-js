  },
  "devDependencies": {
    "@types/chai": "^4.0.1",
    "@types/mocha": "^2.2.41",
    "@types/node": "^14.0.22",
    "@types/node-fetch": "^2.5.7",
    "@types/uuid": "^8.3.0",
