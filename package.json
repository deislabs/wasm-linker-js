    "express": "4.17.1",
    "mocha": "^7.1.1",
    "@rollup/plugin-typescript": "^6.1.0",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.8.1",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  },
  "scripts": {
    "build": "tsc && webpack --debug",
